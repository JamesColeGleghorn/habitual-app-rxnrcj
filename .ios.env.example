
# iOS Environment Configuration
# Copy this file to .ios.env and fill in your values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# OpenRouter Configuration (for code completion feature)
EXPO_PUBLIC_OPENROUTER_API_KEY=your-openrouter-api-key

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_URL=https://api.yourapp.com

# Feature Flags
EXPO_PUBLIC_ENABLE_CODE_COMPLETION=true
EXPO_PUBLIC_ENABLE_WELLNESS_TRACKING=true
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
